{
  "manifest_version": 2,
  "name": "resize1280",
  "version": "1",
  "description": "resize browser width 1280px",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "tabs"  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}